<?xml version="1.0" encoding="utf-8"?>
<!-- $Id: core_administrator.xml 240 2011-02-07 19:45:56Z chdemko $ -->
<form>
	<fields label="COM_LOCALISE_TRANSLATION_DETAIL">
		<field
			name="version"
			type="text"
			size="40"
			class="inputbox"
			default=""
			label="COM_LOCALISE_TRANSLATION_VERSION"
			description="COM_LOCALISE_TRANSLATION_VERSION_DESC"
		/>
		<field
			name="package"
			type="text"
			size="40"
			class="inputbox"
			default=""
			label="COM_LOCALISE_TRANSLATION_VERSION"
			description="COM_LOCALISE_TRANSLATION_VERSION_DESC"
		/>
		<field
			name="subpackage"
			type="text"
			size="40"
			class="inputbox"
			default=""
			label="COM_LOCALISE_TRANSLATION_VERSION"
			description="COM_LOCALISE_TRANSLATION_VERSION_DESC"
		/>
		<field
			name="description"
			readonly="true"
			type="textarea"
			class="inputbox"
			cols="50"
			default="Joomla! English(GB)"
			label="COM_LOCALISE_TRANSLATION_DESCRIPTION"
			description="COM_LOCALISE_TRANSLATION_DESCRIPTION_DESC"
		/>
		<field
			name="date"
			type="text"
			size="40"
			class="inputbox"
			default=""
			label="COM_LOCALISE_TRANSLATION_CREATION_DATE"
			description="COM_LOCALISE_TRANSLATION_CREATION_DATE_DESC"
		/>
		<field
			name="author"
			readonly="true"
			type="text"
			size="40"
			class="inputbox"
			default="Joomla! Project"
			label="COM_LOCALISE_AUTHOR"
			description="COM_LOCALISE_AUTHOR_DESC"
		/>
		<field
			name="copyright"
			readonly="true"
			type="text"
			size="50"
			default="Copyright (C) 2005 - %s Open Source Matters. All rights reserved."
			label="COM_LOCALISE_JOOMLA_COPYRIGHT"
			description="COM_LOCALISE_JOOMLA_COPYRIGHT_DESC"
		/>
		<field
			name="copyright2"
			multiple="true"
			type="text"
			size="50"
			class="inputbox"
			default=""
			label="COM_LOCALISE_TRANSLATION_COPYRIGHT"
			description="COM_LOCALISE_TRANSLATION_COPYRIGHT_DESC"
		/>
		<field
			name="license"
			readonly="true"
			type="text"
			size="50"
			class="inputbox"
			default="http://www.gnu.org/licenses/gpl-2.0.html GNU/GPL"
			label="COM_LOCALISE_TRANSLATION_LICENSE"
			description="COM_LOCALISE_TRANSLATION_LICENSE_DESC"
		/>
	</fields>
</form>
